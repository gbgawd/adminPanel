        path: ':id',\n              meta: {\n                label: 'User Details'\n              },\n              name: 'User',\n              component: User\n            }\n          ]\n        },\n        {\n          path: 'cards',\n          meta: {\n            label: 'Cards'\n          },\n          component: {\n            render(c) {\n              return c('router-view')\n            }\n          },\n          children: [\n            {\n              path: '',\n              name: 'Cards',\n              component: Cards\n            }            \n          ]\n        },   \n        {\n          path: 'editCard',\n          meta: {\n            label: 'Edit Card'\n          },\n          component: {\n            render(c) {\n              return c('router-view')\n            }\n          },\n          children: [\n            {\n              path: '',\n              name: 'Edit Card',\n              component: editCard\n            }            \n          ]\n        },     \n        {\n          path: 'requests',\n          meta: {\n            label: 'Requests'\n          },\n          component: {\n            render(c) {\n              return c('router-view')\n            }\n          },\n          children: [\n            {\n              path: '',\n              name: 'Requests',\n              component: Requests\n            }            \n          ]\n        }      \n      ]\n    },\n    { \n      path: '/pages',\n      redirect: '/pages/404',\n      name: 'Pages',\n      component: {\n        render (c) { return c('router-view') }\n      },\n      children: [\n        {\n          path: '404',\n          name: 'Page404',\n          component: Page404\n        },\n        {\n          path: '500',\n          name: 'Page500',\n          component: Page500\n        },\n        {\n          path: 'login',\n          name: 'Login',\n          component: Login\n        },\n        {\n          path: 'register',\n          name: 'Register',\n          component: Register\n        }\n      ]\n    }\n  ]\n}\n\n"]}]}